{
  "name": "mcp-jppr",
  "version": "1.1.0",
  "description": "MCP server for Junta de PlanificaciÃ³n Puerto Rico (JPPR) property and GIS data APIs. Supports stdio, DXT, and Cloudflare Worker with Auth0 OAuth.",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "node test-catastros.js",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "start:stdio": "node dist/server-stdio.js",
    "dev:worker": "wrangler dev",
    "deploy:worker": "wrangler deploy",
    "clean": "rm -rf dist",
    "build:dxt": "npm run build && node --input-type=module --eval \"import('@alianzacap/mcp-framework').then(f => f.buildDxtAuto())\""
