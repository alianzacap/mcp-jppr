    "hono": "^4.10.3",
    "octokit": "^5.0.4",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2",
    "wrangler": "^4.45.0"
  },
  "keywords": [
    "mcp",
    "puerto-rico",
    "jppr",
    "mipr",
